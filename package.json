{
	"name": "svelte-starter",
	"publisher": "thorcc",
	"displayName": "svelte-starter",
	"description": "",
	"version": "0.0.2",
	"repository": {
		"type": "git",
		"url": "https://github.com/thorcc/svelte-starter-VSCode-extension"
	  },
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:svelte-starter.createSvelte",
		"onCommand:svelte-starter.createSapper",
		"onCommand:svelte-starter.runSvelte"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "svelte-starter.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "svelte-starter.runSvelte",
				"title": "run svelte"
			},
			{
				"command": "svelte-starter.createSvelte",
				"title": "create svelte"
			}
		],
		"menus": {
			"explorer/context": [
				{
				"command": "svelte-starter.createSvelte",
				"group": "SvelteGroup@1"
		  },
		  {
			"command": "svelte-starter.createSapper",
			"group": "SvelteGroup@1" 
		  },
		  {
			"command": "svelte-starter.runSvelte",
			"group": "SvelteGroup@1" 
		  }
		]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
